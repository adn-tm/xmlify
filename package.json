{
  "name": "xmlify",
  "description": "JavaScript/JSON/MongoDb/Mongoose to XML conversion",
  "author": "Chris Veness",
  "version": "1.0.0",
  "license": "MIT",
  "main": "xmlify.js",
  "keywords": [
    "JavaScript",
    "JSON",
    "Mongoose",
    "MongoDb",
    "XML",
    "conversion"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/chrisveness/xmlify.git"
  },
  "bugs": "https://github.com/chrisveness/xmlify/issues",
  "scripts": {
    "test": "mocha test/test.js"
  },
  "dependencies": {
    "inflection": "^1.6.0",
    "xmldom": "^0.1.19"
  },
  "devDependencies": {
    "eslint": "^0.16.2",
    "mocha": "^2.2.1",
    "should": "^5.1.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "curly": 0,
      "eqeqeq": 0,
      "quotes": [ 2, "single", "avoid-escape" ],
      "new-cap": [ 1,  { "capIsNewExceptions": [ "Error" ] } ],
      "no-multi-spaces": 0,
      "no-path-concat": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": "nofunc",
      "space-infix-ops": 0
    }
  }
}
